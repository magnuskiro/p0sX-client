{
  "name": "p0sx-client",
  "productName": "p0sX",
  "version": "0.0.1",
  "description": "p0sX is a Point of Sales application",
  "main": "src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "dist:win64": "npm run package --arch=x64 && build --win --x64 --pd ./out/p0sX-win32-x64",
    "dist:linux64": "electron-forge make",
    "lint": "eslint --ext .js --ext .jsx src"
  },
  "keywords": [],
  "author": {
    "name": "Nuxis",
    "email": "kradalby@kradalby.no"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nuxis/p0sX-client.git"
  },
  "license": "MIT",
  "build": {
    "appId": "org.nuxis.p0sX"
  },
  "config": {
    "forge": {
      "electronPackagerConfig": {
        "asar": true,
        "icon": "./build/icon.ico",
        "ignore": [
          ".idea",
          ".gitignore",
          ".eslintrc",
          ".github",
          ".webpack",
          ".vscode",
          "^/dist$"
        ]
      }
    }
  },
  "devDependencies": {
    "@types/electron": "^1.4.37",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "electron-prebuilt-compile": "1.6.2",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2"
  },
  "dependencies": {
    "axios": "^0.16.1",
    "electron-compile": "^6.4.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-react-devtools": "^0.4.0",
    "electron-settings": "^3.0.14",
    "escpos-print": "^1.1.1",
    "flexboxgrid": "^6.3.1",
    "font-awesome": "^4.7.0",
    "history": "^4.6.1",
    "immutable": "^3.8.1",
    "material-ui": "^0.17.4",
    "moment": "^2.18.1",
    "react": "^15.5.4",
    "react-transition-group": "^1.1.2",
    "react-dom": "^15.5.4",
    "react-notifications": "^1.3.0",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "react-tap-event-plugin": "^2.0.1",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.8",
    "reselect": "^3.0.0",
    "roboto-fontface": "^0.7.0",
    "printj": "^1.0.0"
  }
}
