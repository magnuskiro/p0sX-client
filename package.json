{
  "name": "p0sX",
  "version": "0.0.1",
  "scripts": {
    "postinstall": "install-app-deps",
    "dist": "build",
    "dist:all": "build -d --platform all --arch all",
    "dist:darwin": "build -d --platform darwin --arch x64",
    "dist:win": "npm run dist:win32 && npm run dist:win64",
    "dist:win32": "build -d --platform win32 --arch ia32",
    "dist:win64": "build -d --platform win32 --arch x64",
    "dist:linux": "npm run dist:linux32 && npm run dist:linux64",
    "dist:linux32": "build -d --platform linux --arch ia32",
    "dist:linux64": "build -d --platform linux --arch x64",
    "lint-js": "node ./node_modules/eslint/bin/eslint.js src/**/**.js",
    "lint-jsx": "node ./node_modules/eslint/bin/eslint.js src/**/**.jsx",
    "lint": "npm run lint-js && npm run lint-jsx",
    "electron": "electron ./app",
    "build": "npm run build-main && npm run build-renderer",
    "build-main": "node -r babel-register ./node_modules/webpack/bin/webpack.js --config .webpack/electron.js --progress --profile --colors",
    "build-renderer": "node -r babel-register ./node_modules/webpack/bin/webpack.js --config .webpack/production.js --progress --profile --colors",
    "watch-renderer": "node -r babel-register ./node_modules/webpack/bin/webpack.js --config .webpack/production.js --progress --profile --colors --watch",
    "start": "npm run build && npm run electron"
  },
  "bin": {
    "electron": "./node_modules/.bin/electron"
  },
  "dependencies": {
  },
  "devDependencies": {
    "axios": "^0.13.0",
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-native-modules": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0 ",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "electron-builder": "^5.12.1",
    "electron-prebuilt": "^1.2.7",
    "electron-settings": "^1.1.1",
    "electron-squirrel-startup": "^1.0.0",
    "eslint": "^3.1.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-config-standard-jsx": "^2.0.0",
    "eslint-config-standard-react": "^3.0.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^2.0.0",
    "file-loader": "^0.9.0",
    "history": "^2.1.2",
    "immutable": "^3.8.1",
    "json-loader": "^0.5.4",
    "material-design-icons-iconfont": "^2.0.5",
    "materialize-css": "^0.97.6",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "react-materialize": "^0.14.4",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-saga": "^0.11.0",
    "reselect": "^2.5.3",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "font-awesome": "^4.6.3",
    "empty-string-loader": "^0.1.1",
    "react-notifications": "^1.3.0"
  },
  "build": {
    "app-bundle-id": "org.nuxis.p0sX",
    "productName": "p0sX",
    "iconUrl": "https://github.com/nuxis/p0sX-client/raw/drone/build/icon.ico",
    "osx": {
      "target": [
        "zip"
      ],
      "icon-size": 64,
      "contents": [
        {
          "x": 425,
          "y": 200,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 120,
          "y": 200,
          "type": "file",
          "path": "computed path to artifact, do not specify it - will be overwritten"
        }
      ]
    }
  }
}
